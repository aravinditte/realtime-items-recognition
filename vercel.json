{
  "version": 2,
  "name": "realtime-object-detection",
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "HOST": "0.0.0.0",
    "PORT": "5000"
  },
  "functions": {
    "app.py": {
      "maxDuration": 60
    }
  }
}
